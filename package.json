{
  "name": "@osrs-leagues/discord-bot",
  "version": "2.4.2",
  "description": "The discord bot for the OSRS Leagues discord server.",
  "main": "./src/index.ts",
  "scripts": {
    "build": "tsc",
    "commands": "ts-node ./src/deploy-commands.ts",
    "lint": "eslint src --ext .ts & prettier src --check",
    "lint:fix": "eslint src --ext .ts --fix & prettier src --write",
    "start": "ts-node ./src/index.ts",
    "start:watch": "nodemon -r ./src/index.ts",
    "start:node": "node ./dist/index.js",
    "release": "changeset tag",
    "test": "jest",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/osrs-leagues/discord-bot.git"
  },
  "author": "Allen Kinzalow",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/osrs-leagues/discord-bot/issues"
  },
  "homepage": "https://github.com/osrs-leagues/discord-bot#readme",
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.3",
    "jest": "^27.4.5",
    "lint-staged": "^14.0.1",
    "nodemon": "^3.0.1",
    "prettier": "^2.5.1",
    "sqlite3": "^5.1.6",
    "ts-jest": "^27.1.2"
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@changesets/cli": "^2.26.2",
    "@discordjs/builders": "^0.11.0",
    "@discordjs/rest": "^2.0.1",
    "@types/babel__core": "^7.1.17",
    "@types/node": "^18.11.9",
    "@types/node-cron": "^3.0.9",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "axios": "^1.5.1",
    "discord-api-types": "^0.26.1",
    "discord.js": "^13.5.0",
    "dotenv": "^16.3.1",
    "mysql2": "^3.6.1",
    "node-cron": "^3.0.2",
    "puppeteer": "^21.3.5",
    "sequelize": "^6.33.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": "18.x",
    "yarn": "1.x"
  }
}
